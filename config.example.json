{
  "databases": {
    "my_mysql": {
      "type": "mysql",
      "host": "localhost",
      "port": 3306,
      "database": "myapp",
      "user": "root",
      "password": "${MYSQL_PASSWORD}"
    },
    "my_postgres": {
      "type": "postgresql",
      "host": "localhost",
      "port": 5432,
      "database": "myapp",
      "user": "postgres",
      "password": "${POSTGRES_PASSWORD}",
      "schema": "public"
    },
    "my_postgres_connection_string": {
      "type": "postgresql",
      "connectionString": "${POSTGRES_CONNECTION_STRING}"
    },
    "my_opengauss": {
      "type": "opengauss",
      "host": "localhost",
      "port": 5433,
      "database": "testdb",
      "user": "gaussdb",
      "password": "${OPENGAUSS_PASSWORD}",
      "schema": "public"
    },
    "my_sqlite": {
      "type": "sqlite",
      "database": "/path/to/database.db"
    },
    "my_oracle": {
      "type": "oracle",
      "host": "localhost",
      "port": 1521,
      "user": "system",
      "password": "${ORACLE_PASSWORD}",
      "serviceName": "ORCL"
    },
    "my_mongodb": {
      "type": "mongodb",
      "uri": "mongodb://localhost:27017",
      "database": "myapp",
      "sampleSize": 1000
    },
    "my_mongodb_with_auth": {
      "type": "mongodb",
      "host": "localhost",
      "port": 27017,
      "database": "myapp",
      "user": "admin",
      "password": "${MONGO_PASSWORD}",
      "authSource": "admin"
    },
    "my_redis": {
      "type": "redis",
      "host": "localhost",
      "port": 6379,
      "password": "${REDIS_PASSWORD}",
      "db": 0,
      "maxKeys": 1000,
      "keyPattern": "*"
    },
    "my_memcached": {
      "type": "memcached",
      "servers": ["localhost:11211"]
    },
    "my_kafka": {
      "type": "kafka",
      "brokers": ["localhost:9092"],
      "clientId": "database-schema-mcp"
    },
    "my_rabbitmq": {
      "type": "rabbitmq",
      "host": "localhost",
      "port": 5672,
      "user": "guest",
      "password": "${RABBITMQ_PASSWORD}",
      "vhost": "/"
    },
    "my_elasticsearch": {
      "type": "elasticsearch",
      "node": "http://localhost:9200"
    },
    "my_elasticsearch_with_auth": {
      "type": "elasticsearch",
      "nodes": ["http://localhost:9200"],
      "auth": {
        "username": "elastic",
        "password": "${ELASTICSEARCH_PASSWORD}"
      }
    },
    "my_elasticsearch_cloud": {
      "type": "elasticsearch",
      "cloudId": "${ELASTIC_CLOUD_ID}",
      "auth": {
        "apiKey": "${ELASTIC_API_KEY}"
      }
    },
    "my_etcd": {
      "type": "etcd",
      "hosts": ["localhost:2379"],
      "prefix": "/",
      "maxKeysToScan": 1000
    },
    "my_etcd_with_auth": {
      "type": "etcd",
      "hosts": ["localhost:2379"],
      "username": "root",
      "password": "${ETCD_PASSWORD}"
    },
    "my_clickhouse": {
      "type": "clickhouse",
      "host": "localhost",
      "port": 8123,
      "database": "default",
      "username": "default",
      "password": "${CLICKHOUSE_PASSWORD}"
    },
    "my_milvus": {
      "type": "milvus",
      "address": "localhost:19530"
    },
    "my_milvus_with_auth": {
      "type": "milvus",
      "address": "localhost:19530",
      "username": "root",
      "password": "${MILVUS_PASSWORD}",
      "secure": false
    }
  }
}
